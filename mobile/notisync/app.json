{
  "expo": {
    "name": "notisync",
    "slug": "notisync",
    "scheme": "notisync",
    "orientation": "default",
    "userInterfaceStyle": "automatic",
    "android": {
      "package": "com.faithfulisuh.notisync",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "SYSTEM_ALERT_WINDOW",
        "BIND_NOTIFICATION_LISTENER_SERVICE",
        "FOREGROUND_SERVICE",
        "POST_NOTIFICATIONS",
        "ACCESS_NOTIFICATION_POLICY",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "usesCleartextTraffic": true,
      "allowBackup": false
    },
    "ios": {
      "bundleIdentifier": "com.faithfulisuh.notisync",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-processing",
          "background-fetch"
        ]
      }
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://notisync.com"
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": "https://notisync.com"
      },
      "eas": {
        "projectId": "eacad04e-aaa8-4ecb-a7bd-b70cfc3aee93"
      }
    }
  }
}